<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace="com.kosta.mapper.MovieMapper">
		<insert id="insertMovie" parameterType="com.kosta.dto.MovieDTO">
			insert into filmbox_movie
			values(#{rank}, #{movieCd}, #{movieNm}, #{openDt}, #{image} )
		</insert>
		<select id="getMovieList" resultType="com.kosta.dto.MovieDTO">
			select rank, movieCd, movieNm, openDt, image
			from filmbox_movie
			order by rank;
		</select>
	
    </mapper>
    
    